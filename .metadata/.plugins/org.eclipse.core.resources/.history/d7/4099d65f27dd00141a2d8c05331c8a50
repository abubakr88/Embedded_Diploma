#include <stdio.h>

int main(void) {
int a,mult=1,i=1;
printf ("insert a number to get it's Multiplication Table");
scanf ("%d" , &a);
for (i=1;i<=10;i++)
{
mult = a*i;
printf("%d * %d = mult \n", a , i , mult);
}
}
