#include <stdio.h>
#include <stdlib.h>

typedef struct stack{
	int top;
	StackEntry entry [MAXSTACK];
}Stack;

int main(void) {
Stack s;
StackEntry e;

CreateStack(&s); //for intializing the stack

Push (e, &s);


}

void CreateStack (Stack *ps){
	ps ->top=0;
}

