#include<stdio.h>

int main() {
int n;
	do {
	printf("Input a positive integer: ");
	scanf("%d",&n);
	} while (n<=0);
	static float grade[3];
	grade[0] = grade[2] + grade[5];
	printf("%d",grade[0]);
}
